FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      chemistryProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

chemistryType
{
    chemistrySolver   ode;
    chemistryThermo   pyrolysis;
}

chemistry           on;

initialChemicalTimeStep 1e-7;//1E-7

EulerImplicitCoeffs
{
    cTauChem        1;
    equilibriumRateLimiter off;
}


odeCoeffs
{
    solver          seulex;
    eps             1e-6; //0.05
}

