FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{

    "rho.*"
    {
		solver          diagonal;
    }

    T
    {
		solver          PCG;
		preconditioner  DIC;
		tolerance       1e-09;
		relTol          0;
    }
    
    TFinal
    {
		$T;
		relTol          0;
    }


    Yi
    {
        solver          PBiCG;
        preconditioner  DILU;
        tolerance       1e-8;
        relTol          0;
    }
}